#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/opencollective/bin/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/opencollective/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules:/Users/ailei/Documents/nb/qa/node_modules:/Users/ailei/Documents/nb/node_modules:/Users/ailei/Documents/node_modules:/Users/ailei/node_modules:/Users/node_modules:/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/opencollective/bin/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/opencollective/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/@nuxt/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules:/Users/ailei/Documents/nb/qa/node_modules:/Users/ailei/Documents/nb/node_modules:/Users/ailei/Documents/node_modules:/Users/ailei/node_modules:/Users/node_modules:/node_modules:/Users/ailei/Documents/nb/qa/back-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/opencollective/bin/opencollective.js" "$@"
else
  exec node  "$basedir/../@nuxt/opencollective/bin/opencollective.js" "$@"
fi
